<h1>Lista de Posts</h1>
<table class="table table-bordered">
<thead>
    <tr>
        <th>ID</th>
        <th>Titulo</th>
        <th>Status</th>
    </tr>
</thead>
<tbody>
    {{#each posts}}
     <tr>
        <th>{{id}}</th>
        <th>{{title}}</th>
        <th>{{status}}</th>
         <th><a class="btn btn-info" href="/admin/edit/{{id}}">Editar</th>
    </tr>
    {{/each}}
</tbody>
</table>