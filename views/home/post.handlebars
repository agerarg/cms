<div class="card card-register mx-auto mt-5">
    <div class="card-header"><h1>Single Post</h1></div>
    <div class="card-body">
          <div class="card mb-4">
            <div class="card-body">
              <h2 class="card-title">{{post.title}}</h2>
              <p class="card-text">{{post.body}}
            </div>
            <div class="card-footer text-muted">
              Posted on {{generateTime date "MMMM Do YYYY"}}
            </div>
          </div>
    </div>
</div>